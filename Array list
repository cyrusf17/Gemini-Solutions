import java.util.*;
 class ArrayListExample{
 public static void main(String args[]){
  
 ArrayList al=new ArrayList();  // creating array list
 al.add(1);                // adding elements    
 al.add(2);
 Iterator itr=al.iterator();
 while(itr.hasNext()){
 System.out.println(itr.next());
 }
 }
 }
